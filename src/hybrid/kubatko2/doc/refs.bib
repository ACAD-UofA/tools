@article{kuhlwilm_ancient_2016,
	title = {Ancient gene flow from early modern humans into {Eastern} {Neanderthals}},
	volume = {530},
	copyright = {{\textcopyright} 2016 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	issn = {0028-0836},
	url = {http://www.nature.com/nature/journal/v530/n7591/full/nature16544.html},
	doi = {10.1038/nature16544},
	abstract = {It has been shown that Neanderthals contributed genetically to modern humans outside Africa 47,000{\textendash}65,000 years ago. Here we analyse the genomes of a Neanderthal and a Denisovan from the Altai Mountains in Siberia together with the sequences of chromosome 21 of two Neanderthals from Spain and Croatia. We find that a population that diverged early from other modern humans in Africa contributed genetically to the ancestors of Neanderthals from the Altai Mountains roughly 100,000 years ago. By contrast, we do not detect such a genetic contribution in the Denisovan or the two European Neanderthals. We conclude that in addition to later interbreeding events, the ancestors of Neanderthals from the Altai Mountains and early modern humans met and interbred, possibly in the Near East, many thousands of years earlier than previously thought.},
	language = {en},
	number = {7591},
