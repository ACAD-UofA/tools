	month = oct,
	year = {2011},
	pmid = {21803858},
	pages = {1705--1719},
	file = {Full Text PDF:/home/grg/ownCloud/Zotero/storage/TBQG4J3S/Orlando et al. - 2011 - True single-molecule DNA sequencing of a pleistoce.pdf:application/pdf;Snapshot:/home/grg/ownCloud/Zotero/storage/4URMUR73/1705.html:text/html}
