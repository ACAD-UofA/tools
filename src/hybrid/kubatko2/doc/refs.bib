	file = {PubMed Central Full Text PDF:/home/grg/ownCloud/Zotero/storage/7HRWKBAU/Mei et al. - 2016 - Whole-genome sequencing of the endangered bovine s.pdf:application/pdf}
}

@article{ginolhac_mapdamage:_2011,
