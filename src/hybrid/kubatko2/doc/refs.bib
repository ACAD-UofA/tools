	pages = {573--598},
	file = {Full Text PDF:/home/grg/ownCloud/Zotero/storage/WWTKINCD/Orlando and Cooper - 2014 - Using Ancient DNA to Understand Evolutionary and E.pdf:application/pdf}
}

@article{the_encode_project_consortium_integrated_2012,
	title = {An {Integrated} {Encyclopedia} of {DNA} {Elements} in the {Human} {Genome}},
