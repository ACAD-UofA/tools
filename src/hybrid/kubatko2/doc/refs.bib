	pages = {btt193},
	file = {Full Text PDF:/home/grg/ownCloud/Zotero/storage/WM2AN496/J{\'o}nsson et al. - 2013 - mapDamage2.0 fast approximate Bayesian estimates .pdf:application/pdf;Snapshot:/home/grg/ownCloud/Zotero/storage/ARKU9CRK/bioinformatics.btt193.html:text/html}
