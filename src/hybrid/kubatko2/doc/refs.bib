@article{johnson_mutation_2011,
	title = {Mutation {Rate} {Distribution} {Inferred} from {Coincident} {SNPs} and {Coincident} {Substitutions}},
	volume = {3},
	issn = {, 1759-6653},
	url = {http://gbe.oxfordjournals.org/content/3/842},
	doi = {10.1093/gbe/evr044},
	abstract = {Mutation rate variation has the potential to bias evolutionary inference, particularly when rates become much higher than the mean. We first confirm prior work that inferred the existence of cryptic, site-specific rate variation on the basis of coincident polymorphisms{\textemdash}sites that are segregating in both humans and chimpanzees. Then we extend this observation to a longer evolutionary timescale by identifying sites of coincident substitutions using four species. From these data, we develop analytic theory to infer the variance and skewness of the distribution of mutation rates. Even excluding CpG dinucleotides, we find a relatively large coefficient of variation and positive skew, which suggests that, although most sites in the genome have mutation rates near the mean, the distribution contains a long right-hand tail with a small number of sites having high mutation rates. At least for primates, these quickly mutating sites are few enough that the infinite sites model in population genetics remains appropriate.},
	language = {en},
	urldate = {2016-02-23},
	journal = {Genome Biol Evol},
	author = {Johnson, Philip L. F. and Hellmann, Ines},
	month = jan,
	year = {2011},
	pmid = {21572094},
	keywords = {divergence, polymorphism, population genetics},
	pages = {842--850},
	file = {Full Text PDF:/home/grg/ownCloud/Zotero/storage/X3IRD77W/Johnson and Hellmann - 2011 - Mutation Rate Distribution Inferred from Coinciden.pdf:application/pdf;Snapshot:/home/grg/ownCloud/Zotero/storage/TNQRBXVP/842.html:text/html}
}

@article{ye_alignment_2015,
