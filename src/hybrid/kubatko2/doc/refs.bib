	file = {Full Text PDF:/home/grg/ownCloud/Zotero/storage/VQWBEZNE/Lynch - 2007 - The frailty of adaptive hypotheses for the origins.pdf:application/pdf;Snapshot:/home/grg/ownCloud/Zotero/storage/3F55C2XQ/8597.html:text/html}
}

