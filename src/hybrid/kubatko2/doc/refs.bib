	journal = {Genome Res.},
	author = {Blakesley, Robert W. and Hansen, Nancy F. and Mullikin, James C. and Thomas, Pamela J. and McDowell, Jennifer C. and Maskeri, Baishali and Young, Alice C. and Benjamin, Beatrice and Brooks, Shelise Y. and Coleman, Bradley I. and Gupta, Jyoti and Ho, Shi-Ling and Karlins, Eric M. and Maduro, Quino L. and Stantripop, Sirintorn and Tsurgeon, Cyrus and Vogt, Jennifer L. and Walker, Michelle A. and Masiello, Catherine A. and Guan, Xiaobin and Program, NISC Comparative Sequencing and Bouffard, Gerard G. and Green, Eric D.},
