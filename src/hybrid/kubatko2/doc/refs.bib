	month = aug,
	year = {2003},
	pmid = {12930768},
	pmcid = {PMC1462670},
	pages = {1645--1656},
	file = {PubMed Central Full Text PDF:/home/grg/ownCloud/Zotero/storage/47VC8VP7/Rannala and Yang - 2003 - Bayes estimation of species divergence times and a.pdf:application/pdf}
}

