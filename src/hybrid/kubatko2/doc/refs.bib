	journal = {Theoretical Population Biology},
	author = {Meng, Chen and Kubatko, Laura Salter},
	month = feb,
	year = {2009},
	keywords = {Coalescence, Hybridization, Hybrid speciation, Incomplete lineage sorting, Introgression, phylogenetics},
	pages = {35--45},
	file = {ScienceDirect Full Text PDF:/home/grg/ownCloud/Zotero/storage/E2CQEBGB/Meng and Kubatko - 2009 - Detecting hybrid speciation in the presence of inc.pdf:application/pdf;ScienceDirect Snapshot:/home/grg/ownCloud/Zotero/storage/4KD448IN/S0040580908001111.html:text/html}
