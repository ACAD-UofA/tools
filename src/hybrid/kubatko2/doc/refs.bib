	journal = {Nat Commun},
	author = {Marie-Nelly, Herv{\'e} and Marbouty, Martial and Cournac, Axel and Flot, Jean-Fran{\c c}ois and Liti, Gianni and Parodi, Dante Poggi and Syan, Sylvie and Guill{\'e}n, Nancy and Margeot, Antoine and Zimmer, Christophe and Koszul, Romain},
	month = dec,
