	author = {Kircher, Martin and Kelso, Janet},
	month = jun,
	year = {2010},
	keywords = {ABI/Life Technologies SOLiD, Helicos HeliScope, Illumina Genome Analyzer, Roche/454 GS FLX Titanium, Sanger capillary sequencing},
	pages = {524--536},
	file = {Full Text PDF:/home/grg/ownCloud/Zotero/storage/I3VZT3UV/Kircher and Kelso - 2010 - High-throughput DNA sequencing {\textendash} concepts and limi.pdf:application/pdf;Snapshot:/home/grg/ownCloud/Zotero/storage/5SUDXP4R/abstract\;jsessionid=B8A69D3B806E5BAAB2EC70605C8E509F.html:text/html}
}

