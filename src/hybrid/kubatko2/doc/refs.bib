	pages = {204--209},
	file = {Full Text PDF:/home/grg/ownCloud/Zotero/storage/PPNZT6UA/Laird et al. - 2004 - Hairpin-bisulfite PCR Assessing epigenetic methyl.pdf:application/pdf;Snapshot:/home/grg/ownCloud/Zotero/storage/N4Z7NCKW/204.html:text/html}
