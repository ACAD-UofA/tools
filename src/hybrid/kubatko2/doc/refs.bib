	pages = {557--567},
	file = {Full Text PDF:/home/grg/ownCloud/Zotero/storage/9WS8SNE7/Salzberg et al. - 2012 - GAGE A critical evaluation of genome assemblies a.pdf:application/pdf;Snapshot:/home/grg/ownCloud/Zotero/storage/E7EH44E8/557.html:text/html}
