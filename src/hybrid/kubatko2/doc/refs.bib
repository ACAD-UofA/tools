	abstract = {The study of genomes has been revolutionized by sequencing machines that output many short overlapping substrings (called reads). The task of sequence assembly in practice is to reconstruct long contiguous genome subsequences from the reads. With Next Generation Sequencing (NGS) technologies, assembly software needs to be more accurate, faster, and more memory-efficient due to the problem complexity and the size of the data sets. In this paper, we develop parallel algorithms and compressed data structures to address several computational challenges of NGS assembly. We demonstrate how commonly available multicore architectures can be efficiently utilized for sequence assembly. In all stages (indexing input strings, string graph construction and simplification, extraction of contiguous subsequences) of our software Pasqual, we use shared-memory parallelism to speed up the assembly process. In our experiments with data of up to 6.8 billion base pairs, we demonstrate that Pasqual generally delivers the best tradeoff between speed, memory consumption, and solution quality. On synthetic and real data sets Pasqual scales well on our test machine with 40 CPU cores with increasing number of threads. Given enough cores, Pasqual is fastest in our comparison.},
	number = {5},
	journal = {IEEE Transactions on Parallel and Distributed Systems},
	author = {Liu, Xing and Pande, Pushkar R. and Meyerhenke, Henning and Bader, David A.},
	month = may,
	year = {2013},
	keywords = {Arrays, Assembly, bioinformatics, de novo sequence assembly, Genomics, high-performance bioinformatics, Indexes, Parallel algorithms, Parallel processing, parallel suffix array construction, shared memory parallelism},
	pages = {977--986},
	file = {IEEE Xplore Abstract Record:/home/grg/ownCloud/Zotero/storage/A2DBI5EG/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/home/grg/ownCloud/Zotero/storage/3DE6GDVN/Liu et al. - 2013 - PASQUAL Parallel Techniques for Next Generation G.pdf:application/pdf}
}

@article{weller_exact_2015,
	title = {Exact approaches for scaffolding},
	volume = {16},
	issn = {1471-2105},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4603742/},
	doi = {10.1186/1471-2105-16-S14-S2},
	abstract = {This paper presents new structural and algorithmic results around the scaffolding problem, which occurs prominently in next generation sequencing. The problem can be formalized as an optimization problem on a special graph, the "scaffold graph". We prove that the problem is polynomial if this graph is a tree by providing a dynamic programming algorithm for this case. This algorithm serves as a basis to deduce an exact algorithm for general graphs using a tree decomposition of the input. We explore other structural parameters, proving a linear-size problem kernel with respect to the size of a feedback-edge set on a restricted version of Scaffolding. Finally, we examine some parameters of scaffold graphs, which are based on real-world genomes, revealing that the feedback edge set is significantly smaller than the input size.},
	number = {Suppl 14},
