	pages = {2959--2963},
	file = {Full Text PDF:/home/grg/ownCloud/Zotero/storage/MJJAV88F/Howison et al. - 2013 - Toward a statistically explicit understanding of d.pdf:application/pdf;Snapshot:/home/grg/ownCloud/Zotero/storage/QFQ33VF5/2959.html:text/html}
