	year = {2015},
	pages = {e1005550},
	file = {Lipson et al. - 2015 - Calibrating the Human Mutation Rate via Ancestral .pdf:/home/grg/ownCloud/Zotero/storage/WXR9VX6R/Lipson et al. - 2015 - Calibrating the Human Mutation Rate via Ancestral .pdf:application/pdf}
}

