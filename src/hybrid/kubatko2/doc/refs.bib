@article{bao_aligngraph:_2014,
	title = {{AlignGraph}: algorithm for secondary de novo genome assembly guided by closely related references},
	volume = {30},
	issn = {1367-4803, 1460-2059},
	shorttitle = {{AlignGraph}},
	url = {http://bioinformatics.oxfordjournals.org/content/30/12/i319},
	doi = {10.1093/bioinformatics/btu291},
	abstract = {Motivation: De novo assemblies of genomes remain one of the most challenging applications in next-generation sequencing. Usually, their results are incomplete and fragmented into hundreds of contigs. Repeats in genomes and sequencing errors are the main reasons for these complications. With the rapidly growing number of sequenced genomes, it is now feasible to improve assemblies by guiding them with genomes from related species.
Results: Here we introduce AlignGraph, an algorithm for extending and joining de novo-assembled contigs or scaffolds guided by closely related reference genomes. It aligns paired-end (PE) reads and preassembled contigs or scaffolds to a close reference. From the obtained alignments, it builds a novel data structure, called the PE multipositional de Bruijn graph. The incorporated positional information from the alignments and PE reads allows us to extend the initial assemblies, while avoiding incorrect extensions and early terminations. In our performance tests, AlignGraph was able to substantially improve the contigs and scaffolds from several assemblers. For instance, 28.7{\textendash}62.3\% of the contigs of Arabidopsis thaliana and human could be extended, resulting in improvements of common assembly metrics, such as an increase of the N50 of the extendable contigs by 89.9{\textendash}94.5\% and 80.3{\textendash}165.8\%, respectively. In another test, AlignGraph was able to improve the assembly of a published genome (Arabidopsis strain Landsberg) by increasing the N50 of its extendable scaffolds by 86.6\%. These results demonstrate AlignGraph{\textquoteright}s efficiency in improving genome assemblies by taking advantage of closely related references.
Availability and implementation: The AlignGraph software can be downloaded for free from this site: https://github.com/baoe/AlignGraph.
Contact: thomas.girke@ucr.edu},
	language = {en},
	number = {12},
	urldate = {2015-12-16},
	journal = {Bioinformatics},
	author = {Bao, Ergude and Jiang, Tao and Girke, Thomas},
	month = jun,
	year = {2014},
	pmid = {24932000},
	pages = {i319--i328},
	file = {Full Text PDF:/home/grg/ownCloud/Zotero/storage/33GBRITF/Bao et al. - 2014 - AlignGraph algorithm for secondary de novo genome.pdf:application/pdf;Snapshot:/home/grg/ownCloud/Zotero/storage/CQIAF26I/i319.html:text/html}
