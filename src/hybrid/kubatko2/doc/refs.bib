	month = oct,
	year = {2015},
	keywords = {ancient DNA, DNA extraction, palaeogenomics, ultrashort fragments},
	pages = {n/a--n/a},
	file = {Full Text PDF:/home/grg/ownCloud/Zotero/storage/28M4CW89/Gamba et al. - 2015 - Comparing the performance of three ancient DNA ext.pdf:application/pdf;Snapshot:/home/grg/ownCloud/Zotero/storage/6PBKWVKG/abstract\;jsessionid=173D964F9D6CA20E4AF77C6D55C4D731.html:text/html}
}

