	urldate = {2015-10-26},
	journal = {Ann Appl Stat},
	author = {Fu, Audrey Qiuyan and Genereux, Diane P. and St{\"o}ger, Reinhard and Laird, Charles D. and Stephens, Matthew},
	year = {2010},
	pmid = {21625348},
	pmcid = {PMC3103139},
	keywords = {bisulfite, hairpin},
	pages = {871--892},
	file = {PubMed Central Full Text PDF:/home/grg/ownCloud/Zotero/storage/6QE5KCJV/Fu et al. - 2010 - STATISTICAL INFERENCE OF TRANSMISSION FIDELITY OF .pdf:application/pdf}
}

